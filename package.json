{
  "name": "temp-sandbox",
  "version": "1.0.2",
  "description": "Create a predictable temporary directory that makes it easy to create, list, and delete files.",
  "keywords": [
    "sandbox",
    "temp",
    "tmp",
    "test"
  ],
  "license": "MIT",
  "repository": "chrisblossom/temp-sandbox",
  "homepage": "https://github.com/chrisblossom/temp-sandbox",
  "author": "Chris Blossom",
  "engines": {
    "node": ">=6.9.0",
    "npm": ">=3.10.10"
  },
  "files": [
    "dist/",
    "flow-typed/"
  ],
  "main": "dist/temp-sandbox.js",
  "scripts": {
    "build": "backtrack build --production",
    "check-all": "backtrack check-all",
    "clean": "backtrack clean",
    "dev": "backtrack dev --development",
    "flow": "backtrack flow",
    "format": "backtrack format",
    "lint": "backtrack lint",
    "lint.fix": "backtrack lint.fix",
    "precommit": "backtrack precommit",
    "prepublishOnly": "backtrack prepublishOnly",
    "prepush": "backtrack prepush",
    "test": "backtrack test",
    "test.ci": "backtrack test.ci",
    "test.ci-pretest": "backtrack test.ci-pretest",
    "test.update": "jest --updateSnapshot",
    "test.watch": "jest --watch"
  },
  "lint-staged": {
    "*.{js,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "./.babelrc.js"
    ]
  },
  "devDependencies": {
    "@backtrack/core": "^0.1.20",
    "@backtrack/preset-node-module": "^0.2.1"
  },
  "dependencies": {
    "del": "^3.0.0",
    "make-dir": "^1.3.0",
    "parent-module": "^0.1.0",
    "read-dir-deep": "^1.0.4",
    "read-pkg-up": "^4.0.0",
    "slash": "^2.0.0",
    "util.promisify": "^1.0.0"
  }
}
